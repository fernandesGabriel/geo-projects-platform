{
  "app": "geo-projects-service-api",
  "primary_region": "fra",
  "kill_signal": "SIGINT",
  "kill_timeout": 60,
  "build": {
    "dockerfile": "Dockerfile",
    "build-target": "app-service-api"
  },
  "http_service": {
    "internal_port": 3000,
    "force_https": true,
    "auto_stop_machines": true,
    "auto_start_machines": true,
    "min_machines_running": 0
  },
  "vm": [
    {
      "cpu_kind": "shared",
      "memory": "1gb",
      "cpus": 1
    }
  ]
}
